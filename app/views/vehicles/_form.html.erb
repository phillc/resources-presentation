<% form_for(@vehicle) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :vin %><br />
    <%= f.text_field :vin %>
  </p>
  <p>
    <%= f.label :year %><br />
    <%= f.text_field :year %>
  </p>
  <p>
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </p>
  <p>
    <%= f.submit 'Submit' %>
  </p>
<% end %>
